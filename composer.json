{
    "name": "typhonius/acquia-logstream",
    "description": "A tool for connecting to Acquia logstream",
    "authors": [
        {
            "name": "Adam Malone",
            "email": "adam@adammalone.net"
        }
    ],
    "license": "MIT",
    "require": {
        "typhonius/acquia-php-sdk-v2": "^2.0.0",
        "ratchet/pawl": "^0.3"
    },
    "autoload":{
        "psr-4":{
            "AcquiaLogstream\\":"src"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.1"
    },
    "scripts": {
        "lint": "php -l ./src/",
        "sniff": "./vendor/bin/phpcs -s --standard=PSR2 ./src/"
    }
}
